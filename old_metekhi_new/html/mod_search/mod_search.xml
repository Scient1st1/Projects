    <version>1.0.0</version>
    <description>This module will display a search box</description>
    <files>
        <filename module="mod_search">mod_search.php</filename>
    </files>
    <params>
      <param name="text" type="text" default="" label="Search Text" description="Search Text Desc" />
       <param name="kbd" type="radio" default="1" label="geo kbd" description="geo kbd on off">
      <option value="0">OFF</option>
      <option value="1">on</option>
    </param>
    </params>
   
   <positions>
        <position>search</position>
		<position>right</position>
    </positions>
